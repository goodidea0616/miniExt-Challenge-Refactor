{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2018",
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "declaration": true,
        "noEmit": true,
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "useUnknownInCatchVariables": false
    },
    "include": [
        "**/*.tsx",
        "**/*.ts"
    ],
}